[package]
name = "greylocker_zkp"
version = "0.1.0"
description = "Zero-knowledge proofs for Greylocker privacy"
edition = "2021"

[lib]
crate-type = ["cdylib", "lib"]
name = "greylocker_zkp"

[features]
no-entrypoint = []
no-idl = []
no-log-ix-name = []
cpi = ["no-entrypoint"]
default = []

[dependencies]
anchor-lang = "0.28.0"
solana-program = "1.18.0"
